	<queryString>
		<![CDATA[select distinct rc.nombre_medicina, rc.resultados, rc.estudio, dv.id_paciente, pac.nombre, pac.fecha_nacimiento, pac.edad, pac.sexo, pac.fecha_ingreso,pac.medico, v.fecha_reporte, v.nota,
rc.nombre_bacteria , rc.porcentaje
from venta v 
inner join descripcion_de_venta dv on v.id_venta = dv.id_venta 
inner join pacientes pac on dv.id_paciente = pac.id_paciente 
inner join resultados_cultivos rc on v.id_venta = rc.id_venta
 
where v.id_venta = $P{parameter1} and v.estado_venta='Realizada'
and SUBSTRING(dv.nombre_producto,1,7) not in ('PAQUETE') 
Order by rc.estudio, rc.nombre_bacteria]]>
	</queryString>
